from selenium import webdriver
from selenium.webdriver.common.by import By
import time


link = "http://suninjuly.github.io/file_input.html"
first_name = "firstname"    # name
last_name = "lastname"      # name
email = "Enter email"       #text in placeholder
send_file_button = "input#file"
button_submit = ".btn"

try:
    browser = webdriver.Chrome()
    browser.get(link)

    name1 = browser.find_element(By.CSS_SELECTOR, first_name)
    name1.send_keys("Eliza")

    name2 = browser.find_element(By.CSS_SELECTOR, last_name)
    name2.click("Rut")

    mail = browser.find_element(By.P, email)
    mail.click(test_mail@mail.ru)

    file = browser.find_element(By.CSS_SELECTOR, )
    name2.click()

    button = browser.find_element(By.TAG_NAME, "button")
    button.click()

finally:
    time.sleep(5)
    browser.quit()
